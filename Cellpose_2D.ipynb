        "\n",
        "if Channel_to_segment == \"Red\":\n",
        "  segment_channel = 1\n",
        "\n",
        "if Nuclear_channel == \"Blue\":\n",
        "  nuclear_channel = 3\n",
        "\n",
        "if Nuclear_channel == \"Green\":\n",
